      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">// Assuming the employee's birthdate and hire date are in string format
var birthDateString = token.current.birthday; // Employee's birthdate
var entryDateString = token.current.entryDate;   // Employee's hire date

// Convert strings to Date objects
var birthDate = new Date(birthDateString);
var entryDate = new Date(entryDateString);

// Get the birthdate for the current year
var birthDateThisYear = new Date(entryDate.getFullYear(), birthDate.getMonth(), birthDate.getDate());

// Check if the birthdate for the current year is before the hire date
if (birthDateThisYear &lt; entryDate) {
    return true;
} else {
    return false;
}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
